<?xml version="1.0" encoding="utf-8" ?>
<WebElement>

  <Name>TutorialCatalog</Name>
  <Description>Tutorial catalog frame</Description>
  <Locator>//div[@id='main']</Locator>

  <ChildWebElements>
    <WebElement>
      <Name>EditContainerFinder</Name>
      <Description>Edit button for container finder</Description>
      <Locator>(//button[.='Edit'])[2]</Locator>
    </WebElement>
    <WebElement>
      <Name>DropContainerFinder</Name>
      <Description>Drag place for container finder</Description>
      <Locator>(//div[contains(@class, 'cq-editrollover-insert-container')])[1]</Locator>
    </WebElement>
  </ChildWebElements>
</WebElement>