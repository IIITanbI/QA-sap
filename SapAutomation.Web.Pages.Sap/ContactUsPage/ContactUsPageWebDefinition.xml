<?xml version="1.0" encoding="utf-8" ?>
<WebElement>
  <Name>NewsletterPageBody</Name>
  <Description>Newsletter form container</Description>
  <Locator>
    <IsRelative>False</IsRelative>
    <XPath>//div[@class='guideContainerNode']</XPath>
  </Locator>

  <ChildWebElements>
    <WebElement>
      <Name>WritingAboutDropdown</Name>
      <Description>Dropdown for writing about selector</Description>
      <Locator>.//*[contains(@name, 'panelTop-TopicArea')]</Locator>
      <ChildWebElements>
        <WebElement>
          <Name>WritingAboutDropdownOption</Name>
          <Description>Option for writing about dropdown</Description>
          <Locator>.//option[@value='toReplace']</Locator>
        </WebElement>
      </ChildWebElements>
    </WebElement>
    <WebElement>
      <Name>IndustryDropdown</Name>
      <Description>Dropdown for industry selector</Description>
      <Locator>.//*[contains(@name, 'panelTop-Industry')]</Locator>
      <ChildWebElements>
        <WebElement>
          <Name>IndustryDropdownOption</Name>
          <Description>Option for writing about dropdown</Description>
          <Locator>.//option[@value='toReplace']</Locator>
        </WebElement>
      </ChildWebElements>
    </WebElement>
    <WebElement>
      <Name>SAPSolutionOfInterestDropdown</Name>
      <Description>Dropdown for sap solution of interest selector</Description>
      <Locator>.//*[contains(@name, 'panelTop-SolutionCurrent')]</Locator>
      <ChildWebElements>
        <WebElement>
          <Name>SAPSolutionOfInterestDropdownOption</Name>
