      </ChildWebElements>
    </WebElement>
    <WebElement>
      <Name>MessageField</Name>
      <Description>Text field for you message</Description>
      <Locator>.//*[contains(@name, 'panelTop-MessageText')]</Locator>
    </WebElement>
    <WebElement>
      <Name>OfficeLocationDropdown</Name>
