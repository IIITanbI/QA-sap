      <Locator>(.//*[@class='guidefield guideradiobutton'])[1]</Locator>
      <ChildWebElements>
        <WebElement>
          <Name>EmailRadioButtonYes</Name>
          <Description>Option for salutation dropdown</Description>
          <Locator>(.//*[contains(@name,'panelBottom-Emailoptin')])[1]</Locator>
        </WebElement>
        <WebElement>
          <Name>EmailRadioButtonNo</Name>
          <Description>Option for salutation dropdown</Description>
          <Locator>(.//*[contains(@name,'panelBottom-Emailoptin')])[2]</Locator>
